{
  "name": "bfl-api",
  "version": "1.7.1",
  "description": "Black Forest Labs API wrapper - FLUX and Kontext image generation for Node.js with CLI",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "type": "module",
  "bin": {
    "bfl": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "bfl": "tsx src/cli.ts",
    "bfl:help": "tsx src/cli.ts --help",
    "bfl:examples": "tsx src/cli.ts --examples",
    "bfl:credits": "tsx src/cli.ts --credits",
    "bfl:dev": "tsx src/cli.ts --flux-dev",
    "bfl:pro": "tsx src/cli.ts --flux-pro",
    "bfl:ultra": "tsx src/cli.ts --flux-ultra",
    "bfl:fill": "tsx src/cli.ts --flux-fill",
    "bfl:expand": "tsx src/cli.ts --flux-expand",
    "bfl:kontext-pro": "tsx src/cli.ts --kontext-pro",
    "bfl:kontext-max": "tsx src/cli.ts --kontext-max",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "bfl",
    "black-forest-labs",
    "flux",
    "flux-dev",
    "flux-pro",
    "flux-ultra",
    "kontext",
    "image-generation",
    "ai-image",
    "text-to-image",
    "image-to-image",
    "cli",
    "typescript"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/bfl-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/bfl-api/issues"
  },
  "homepage": "https://github.com/aself101/bfl-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.10",
    "semantic-release": "^24.2.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
